{
  "name": "@kinfo-dev/vue-pdf",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dombaigabor/vue-pdf.git"
  },
  "keywords": [
    "vue.js",
    "pdf",
    "viewer"
  ],
  "author": "Gabor Dombai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dombaigabor/vue-pdf/issues"
  },
  "homepage": "https://github.com/dombaigabor/vue-pdf",
  "version": "1.0.5",
  "description": "vue.js pdf viewer (forked from FranckFreiburger/vue-pdf)",
  "main": "dist/vue-pdf.ssr.js",
  "browser": "dist/vue-pdf.esm.js",
  "module": "dist/vue-pdf.esm.js",
  "unpkg": "dist/vue-pdf.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vite serve ./dev",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts"
  },
  "dependencies": {
    "pdfjs-dist": "2.11.338"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vue/compiler-sfc": "^3.2.21",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.59.0",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-visualizer": "5.5.2",
    "rollup-plugin-vue": "^5.1.9",
    "ts-jest": "^27.0.7",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.4",
    "vite": "2.6.13",
    "vite-plugin-vue2": "1.9.0",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-resize-sensor": "^2.0.0",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
